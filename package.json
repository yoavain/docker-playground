{
  "name": "docker-playground",
  "version": "1.0.0",
  "description": "Learning Docker",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "predocker:build": "npm run build && docker rm -f docker-playground && docker rmi -f docker-playground",
    "docker:build": "docker build -t docker-playground .",
    "postdocker:build": "docker scan docker-playground",
    "predocker:start": "npm run docker:build",
    "docker:start": "docker run -dp 3000:3000 --name docker-playground docker-playground",
    "docker:stop": "docker stop docker-playground"
  },
  "dependencies": {
    "express": "4.17.3"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.1",
    "@types/node": "17.0.23",
    "rimraf": "3.0.2",
    "typescript": "4.6.3"
  }
}
